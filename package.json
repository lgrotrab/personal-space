{
  "name": "personal-space",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "seed": "node -r dotenv/config ./infra/database-mock-data.js dotenv_config_path=./.env.local",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "container": "docker compose -f ./infra/compose.yaml --env-file ./.env.local up -d ",
    "stop:container": "docker compose -f ./infra/compose.yaml --env-file ./.env.local down "
  },
  "dependencies": {
    "mongoose": "^7.6.4",
    "next": "13.5.5",
    "react": "^18",
    "react-dom": "^18"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.2.0",
    "autoprefixer": "^10",
    "dotenv": "^16.3.1",
    "eslint": "^8",
    "eslint-config-next": "13.5.5",
    "eslint-config-prettier": "^9.0.0",
    "jest": "^29.7.0",
    "postcss": "^8",
    "prettier": "^3.0.3",
    "tailwindcss": "^3"
  }
}
